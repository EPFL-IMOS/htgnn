    "version": "0.2.0",
    "jupyter.debugJustMyCode": false,
    "configurations": [
        {
            "name": "Python: Train HTGNN Bearing Load",
            "type": "debugpy",
            "request": "launch",
            "module": "run.main",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "--cfg",
                "run/configs/bearing/htgnn.yaml",
                "--repeat",
                "1",
            ]
        },
        {
            "name": "Python: Train HTGNN Bridge Load",
            "type": "debugpy",
            "request": "launch",
            "module": "run.main",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "--cfg",
                "run/configs/bridge/htgnn.yaml",
                "--repeat",
                "1",
            ]
        },
        {
            "name": "Python: Train TimeMixer Bearing Load",
            "type": "debugpy",
            "request": "launch",
            "module": "run.main",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "args": [
                "--cfg",
                "run/configs/bearing/timemixer.yaml",
                "--repeat",
                "1",
            ]
        },
        {
            "name": "Python",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
        }
    ]
}
